<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstcomponents"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_OWNER_DATA" />
    <uses-permission android:name="android.permission.READ_OWNER_DATA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="my_activities.MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="my_activities.BasicFragmentActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity android:name="my_components.tag.TagActivity" >
        </activity>
        <activity android:name="my_activities.TagActivity" >
        </activity>
        <activity android:name="my_activities.SensoresActivity" >
        </activity>
        <activity
            android:name="my_activities.CoordCommentActivity"
            android:configChanges="orientation|screenSize" >
        </activity>
        <activity android:name="my_activities.SavePhotoActivity" >
        </activity>
        <activity android:name="my_activities.Rating2Comment2Photo" >
        </activity>
        <activity
            android:name="my_activities.NewListComposed"
            android:configChanges="orientation|screenSize" >
        </activity>
        <activity
            android:name="my_activities.NewListActivity"
            android:configChanges="orientation|screenSize" >
        </activity>
        <activity
            android:name="my_activities.FaqTestActivity"
            android:configChanges="orientation|screenSize" >
        </activity>
        <activity android:name="my_activities.ImageZoomActivity" >
        </activity>

        <service
            android:name="com.gw.android.components.sensor_service.SensorManagerService"
            android:enabled="true" />
    </application>

</manifest>